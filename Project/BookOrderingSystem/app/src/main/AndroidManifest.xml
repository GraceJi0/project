<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="comp3350project.bookorderingsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presentation.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentation.SearchActivity">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentation.ViewBookActivity"></activity>
        <activity android:name=".presentation.ManagerMainActivity"></activity>
        <activity android:name=".presentation.ManagerViewBooksActivity"></activity>
        <activity android:name=".presentation.ManagerViewCustomerActivity"></activity>
        <activity android:name=".presentation.EditBookActivity"></activity>
        <activity android:name=".presentation.SearchCategoryActivity"></activity>
    </application>

</manifest>